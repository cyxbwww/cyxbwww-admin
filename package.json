{
  "name": "vue-blog-manager",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "cross-env VITE_ENV_TYPE=dev vite",
    "dev:test": "cross-env VITE_ENV_TYPE=test vite",
    "dev:prod": "cross-env VITE_ENV_TYPE=prod vite",
    "build": "vue-tsc --noEmit && cross-env VITE_ENV_TYPE=dev vite build",
    "build:test": "vue-tsc --noEmit && cross-env VITE_ENV_TYPE=test vite build",
    "build:prod": "vue-tsc --noEmit && cross-env VITE_ENV_TYPE=prod vite build",
    "preview": "vite preview",
    "lint": "eslint src --fix --ext .ts,.vue,.js",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "colord": "^2.9.3",
    "crypto-js": "^4.1.1",
    "form-data": "^4.0.0",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.33.1",
    "pinia": "^2.0.21",
    "qs": "^6.11.0",
    "ua-parser-js": "^1.0.2",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@iconify/vue": "^3.2.1",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^18.7.14",
    "@types/qs": "^6.9.7",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "commitizen": "^4.2.5",
    "commitlint-config-cz": "^0.13.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.9.2",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.4.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rollup-plugin-visualizer": "^5.8.0",
    "sass": "^1.54.7",
    "typescript": "^4.8.2",
    "unocss": "^0.45.13",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.4",
    "vfonts": "^0.0.3",
    "vite": "^3.0.9",
    "vue-tsc": "^0.40.5"
  },
  "lint-staged": {
    "*.{js,ts,vue,css,scss}": [
      "npm run lint",
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
