{
  "name": "vue-blog-manager",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "cross-env VITE_ENV_TYPE=dev vite",
    "dev:test": "cross-env VITE_ENV_TYPE=test vite",
    "dev:prod": "cross-env VITE_ENV_TYPE=prod vite",
    "build": "vue-tsc --noEmit && cross-env VITE_ENV_TYPE=dev vite build",
    "build:test": "vue-tsc --noEmit && cross-env VITE_ENV_TYPE=test vite build",
    "build:prod": "vue-tsc --noEmit && cross-env VITE_ENV_TYPE=prod vite build",
    "preview": "vite preview",
    "lint": "eslint src --fix --ext .ts,.vue,.js",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "colord": "^2.9.2",
    "crypto-js": "^4.1.1",
    "form-data": "^4.0.0",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.30.3",
    "pinia": "^2.0.14",
    "qs": "^6.10.5",
    "ua-parser-js": "^1.0.2",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@iconify/vue": "^3.2.1",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^17.0.42",
    "@types/qs": "^6.9.7",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "prettier": "^2.6.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.52.3",
    "typescript": "^4.7.3",
    "unocss": "^0.38.2",
    "unplugin-auto-import": "^0.8.7",
    "unplugin-vue-components": "^0.19.6",
    "vfonts": "^0.0.3",
    "vite": "^2.9.12",
    "vue-tsc": "^0.37.3"
  },
  "lint-staged": {
    "*.{js,ts,vue,css,scss}": [
      "npm run lint",
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
